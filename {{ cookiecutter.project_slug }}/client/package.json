    "generate-env-config": "chmod +x ./env.sh && ./env.sh && cp ./src/generated/env-config.js ./public/",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.ts,.tsx src/ --fix",
    "start": "concurrently \"npm run generate-env-config && webpack-dev-server --progress --color --mode development --config webpack.config.development.js\" \"npm run generate:watch\"",
    "prebuild": "npm run generate",
    "build": "chmod +x ./env.sh && ./env.sh && cp ./src/generated/env-config.js ./public/ && webpack --color --mode production --config webpack.config.production.js",
